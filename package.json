{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "sourcegraph",
  "title": "Sourcegraph",
  "description": "Universal Code Search - move fast, even in big codebases.",
  "icon": "command-icon.png",
  "author": "bobheadxi",
  "license": "MIT",
  "keywords": ["search", "code", "sourcegrpah", "codesearch"],
  "commands": [
    {
      "name": "search",
      "title": "Search Sourcegraph",
      "description": "Search your code and 2M+ public repositories.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.27.0",
    "@bobheadxi/node-eventsource-http2": "^1.1.1",
    "node-fetch": "^3.0.0",
    "sourcegraph": "^25.4.0"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "react-devtools": "~4.13.0",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "lint": "ray lint"
  }
}
